<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/white"
    android:layout_margin="10dp"
    android:orientation="vertical"
    >



    <Button
        android:id="@+id/calculateNamta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Calculate Namta"/>


    <Button
        android:id="@+id/calculateEven"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Calculate Even"/>

    <Button
        android:id="@+id/calculateSumSeries"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Calculate Sum Series"/>


    <Button
        android:id="@+id/layoutInflateBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="inflate layout"/>

    <Button
        android:id="@+id/btnListview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Listview "/>

    <Button
        android:id="@+id/btnServerReq"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Req Server"/>

    <Button
        android:id="@+id/btnFregment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GO to Fregment"/>







</LinearLayout>